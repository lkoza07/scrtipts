local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Scripts", "Synapse")

--Dahood
local Main = Window:NewTab("Dahood")
local MainSection = Main:NewSection("Dahood")

MainSection:NewButton("SwagMode", "", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/lerkermer/lua-projects/master/SwagModeV002"))()
end)

MainSection:NewButton("Destiny", "", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/compwnter/destiny/main/loader'))();
end)

MainSection:NewButton("Raycodex", "", function()
    loadstring(game:GetObjects("rbxassetid://5812737894" )[1].Source)()
end)

MainSection:NewButton("Nova X", "", function()
    loadstring(game:HttpGet("https://icxy.xyz/NovaGui/NovaGui", true))()
end)

MainSection:NewButton("Hood Shark", "", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/vgVZ5JWN'))();
end)

MainSection:NewButton("Fadded", "", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/NighterEpic/Faded/main/YesEpic", true))()
end)

MainSection:NewButton("Arctic", "", function()
    loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/PolarWasHere/Arctic/main/Arctic"))()
end)




