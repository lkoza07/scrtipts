--Other
local Other = Window:NewTab("Other")
local OtherSection = Other:NewSection("Other scripts")

OtherSection:NewButton("Ragdoll", "", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/martinelcrac/cryptonichub/main/Ragdollengine.lua'))()
end)

OtherSection:NewButton("Chaos", "", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/1111-ssss/Chaos_Script/main/Chaos_script.txt'))()
end)

OtherSection:NewButton("Invisible Boombox", "Da hood", function()
    game:GetService("Players").LocalPlayer.Character:FindFirstChild("BOOMBOXHANDLE"):Destroy()
end)

OtherSection:NewButton("v sim", "", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/zt0V6Rg2'))()
end)

OtherSection:NewButton("Korblox & Headless", "NOT FE", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/8cM96TDW'))()
end)

OtherSection:NewButton("Headless", "NOT FE", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/Bh8PTLpv'))()
end)

OtherSection:NewButton("Korblox", "NOT FE", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/MankQPX0'))()
end)

OtherSection:NewToggle("120 walkspeed", "", function(state)
    if state then
        getgenv().WalkSpeedValue = 120; --set your desired walkspeed here
local Player = game:service'Players'.LocalPlayer;
Player.Character.Humanoid:GetPropertyChangedSignal'WalkSpeed':Connect(function()
Player.Character.Humanoid.WalkSpeed = getgenv().WalkSpeedValue;
end)
Player.Character.Humanoid.WalkSpeed = getgenv().WalkSpeedValue;
    else
        getgenv().WalkSpeedValue = 16; --set your desired walkspeed here
local Player = game:service'Players'.LocalPlayer;
Player.Character.Humanoid:GetPropertyChangedSignal'WalkSpeed':Connect(function()
Player.Character.Humanoid.WalkSpeed = getgenv().WalkSpeedValue;
end)
Player.Character.Humanoid.WalkSpeed = getgenv().WalkSpeedValue;
    end
end)

OtherSection:NewToggle("50 walkspeed", "", function(state)
    if state then
        getgenv().WalkSpeedValue = 50; --set your desired walkspeed here
local Player = game:service'Players'.LocalPlayer;
Player.Character.Humanoid:GetPropertyChangedSignal'WalkSpeed':Connect(function()
Player.Character.Humanoid.WalkSpeed = getgenv().WalkSpeedValue;
end)
Player.Character.Humanoid.WalkSpeed = getgenv().WalkSpeedValue;
    else
        getgenv().WalkSpeedValue = 16; --set your desired walkspeed here
local Player = game:service'Players'.LocalPlayer;
Player.Character.Humanoid:GetPropertyChangedSignal'WalkSpeed':Connect(function()
Player.Character.Humanoid.WalkSpeed = getgenv().WalkSpeedValue;
end)
Player.Character.Humanoid.WalkSpeed = getgenv().WalkSpeedValue;
    end
end)





























